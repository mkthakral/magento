                $j('#preview2').attr('src', e.target.result);
				
				//$wk_jq("#imageTemplate").tmpl([{'index':i}]).appendTo("#otherimages");
            }
//alert(input.files[0]);
            reader.readAsDataURL(input.files[0]);
        }
    }

</script>
